	penumbraShadow.ImageTransparency = 0.880

	ambientShadow.Name = "ambientShadow"
	ambientShadow.Parent = shadowHolder
	ambientShadow.AnchorPoint = Vector2.new(0.5, 0.5)
	ambientShadow.BackgroundTransparency = 1.000
	ambientShadow.Position = UDim2.new(0.5, 0, 0.5, 5)
	ambientShadow.Size = UDim2.new(1, 18, 1, 18)
	ambientShadow.ZIndex = 0
	ambientShadow.Image = "rbxassetid://1316045217"
	ambientShadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
	ambientShadow.ImageTransparency = 0.880
	
	local correctsound = Instance.new("Sound")
	correctsound.Name = "correct sound"
	correctsound.Parent = KeySystem
	correctsound.SoundId = "rbxassetid://3642623722"
	correctsound.Volume = 1
	correctsound.PlaybackSpeed = 1.5
	correctsound.RollOffMaxDistance = 100
	correctsound.RollOffMinDistance = 10
	correctsound.RollOffMode = Enum.RollOffMode.Linear

	local errorsound = Instance.new("Sound")
	errorsound.Name = "error sound"
	errorsound.Parent = KeySystem
	errorsound.SoundId = "rbxassetid://136075117"
	errorsound.Volume = 1
	errorsound.PlaybackSpeed = 1.5
	errorsound.RollOffMaxDistance = 100
	errorsound.RollOffMinDistance = 10
	errorsound.RollOffMode = Enum.RollOffMode.Linear
	
	
	-- Function to validate and process key input
	local function processKeyInput()
		local keyInput = TextBox.Text

		-- Check if the entered key is valid
		local valid = false
		for _, key in pairs(validKey) do
			if key == keyInput then
				valid = true
				break
			end
		end

		if valid then
			-- Perform action for valid key
			
			TextLabel_6.TextColor3 = Color3.fromRGB(0, 255, 0)
			TextLabel_6.Text = "Key Accepted!"
		else
			-- Handle invalid key input
			TextLabel_6.TextColor3 = Color3.fromRGB(255, 0, 0)
			TextLabel_6.Text = "Invalid Key. Please try again."
		end
	end

	-- Connect the button to process key input
	EnterKey.MouseButton1Click:Connect(processKeyInput)
	
	
	-- Functions:
	local function ValidateKey(input)
		-- Check if the input matches the validKey
		if input == validKey then
			return true
		else
			return false
		end
	end

	-- Whitelist check function
	local function IsInWhitelist(player)
		if whitelist[player.UserId] then
			return true
		else
			return false
		end
	end


	-- Handle enter key press event for input
	TextBox.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			processKeyInput()
		end
	end)


	GetKey.MouseButton1Click:Connect(function()
		if Link then
			-- This would be handled by the player's browser
			setclipboard(Link) -- Copies the link to the clipboard so the player can paste it into their browser
			print("Discord link copied to clipboard.")
		end
	end)

	JoinDiscord.MouseButton1Click:Connect(function()
		if Link then
			-- This would be handled by the player's browser
			setclipboard(dislink) -- Copies the link to the clipboard so the player can paste it into their browser
			print("Discord link copied to clipboard.")
		end
	end)

	Close.MouseButton1Click:Connect(function()
		KeySystem:Destroy()
	end)
end

return Library
