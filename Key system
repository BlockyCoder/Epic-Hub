local Library = {}

function Library:Notify(Name, Desc, Duration)
    local ScreenGui = Instance.new("ScreenGui")
    local Main = Instance.new("Frame")
    local Title = Instance.new("TextLabel")
    local desc = Instance.new("TextLabel")
    local UICorner = Instance.new("UICorner")

    ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

    Main.Name = "Main"
    Main.Parent = ScreenGui
    Main.BackgroundColor3 = Color3.fromRGB(23, 23, 25)
    Main.BorderColor3 = Color3.fromRGB(0, 0, 0)
    Main.BorderSizePixel = 0
    Main.Size = UDim2.new(0, 360, 0, 124)
    Main.Position = UDim2.new(1, -360, 0.789889395, 0)

    Title.Name = "Title"
    Title.Parent = Main
    Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Title.BackgroundTransparency = 1.000
    Title.BorderColor3 = Color3.fromRGB(0, 0, 0)
    Title.BorderSizePixel = 0
    Title.Size = UDim2.new(1, 0, 0, 37)
    Title.Font = Enum.Font.SourceSans
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextScaled = true
    Title.TextSize = 14.000
    Title.TextWrapped = true
    Title.Text = Name

    desc.Name = "desc"
    desc.Parent = Main
    desc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    desc.BackgroundTransparency = 1.000
    desc.BorderColor3 = Color3.fromRGB(0, 0, 0)
    desc.BorderSizePixel = 0
    desc.Size = UDim2.new(1, 0, 0, 70)
    desc.Position = UDim2.new(0, 0, 0.435483873, 0)
    desc.Font = Enum.Font.SourceSans
    desc.TextColor3 = Color3.fromRGB(255, 255, 255)
    desc.TextScaled = true
    desc.TextSize = 14.000
    desc.TextWrapped = true
    desc.Text = Desc

    UICorner.CornerRadius = UDim.new(0, 12)
    UICorner.Parent = Main

    local function animateNotification()
        local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quint, Enum.EasingDirection.Out, 0, false)
        local goal = {
            Position = UDim2.new(0.681170881, 0, 0.789889395, 0)
        }
        local tween = game:GetService("TweenService"):Create(Main, tweenInfo, goal)
        tween:Play()
    end

    local function removeNotification()
        wait(Duration)
        local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quint, Enum.EasingDirection.In, 0, false)
        local goal = {
            Position = UDim2.new(1, 0, 0.789889395, 0)
        }
        local tween = game:GetService("TweenService"):Create(Main, tweenInfo, goal)
        tween:Play()
        tween.Completed:Connect(function()
            ScreenGui:Destroy()
        end)
    end

    animateNotification()
    spawn(removeNotification)
end

function Library:KeyS(KName, validKey, Link, dislink, callback, whitelist)
    local KeySystem = Instance.new("ScreenGui")
    local Frame = Instance.new("Frame")
    local Topbar = Instance.new("Frame")
    local Frame_2 = Instance.new("Frame")
    local TextLabel = Instance.new("TextLabel")
    local UIPadding = Instance.new("UIPadding")
    local Close = Instance.new("ImageButton")
    local Container = Instance.new("Frame")
    local TextLabel_2 = Instance.new("TextLabel")
    local TextboxHolder = Instance.new("Frame")
    local UICorner = Instance.new("UICorner")
    local TextBox = Instance.new("TextBox")
    local UIPadding_2 = Instance.new("UIPadding")
    local TextLabel_3 = Instance.new("TextLabel")
    local JoinDiscord = Instance.new("TextButton")
    local UICorner_2 = Instance.new("UICorner")
    local EnterKey = Instance.new("TextButton")
    local UICorner_3 = Instance.new("UICorner")
    local GetKey = Instance.new("TextButton")
    local UICorner_4 = Instance.new("UICorner")
    local shadowHolder = Instance.new("Frame")
    local umbraShadow = Instance.new("ImageLabel")
    local penumbraShadow = Instance.new("ImageLabel")
    local ambientShadow = Instance.new("ImageLabel")

    KeySystem.Name = "Key System"
    KeySystem.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

    Frame.Parent = KeySystem
    Frame.AnchorPoint = Vector2.new(0.5, 0.5)
    Frame.BackgroundColor3 = Color3.fromRGB(23, 23, 25)
    Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
    Frame.BorderSizePixel = 0
    Frame.Position = UDim2.new(0.5, 0, 0.5, 0)
    Frame.Size = UDim2.new(0, 325, 0, 190)

    Topbar.Name = "Topbar"
    Topbar.Parent = Frame
    Topbar.BackgroundColor3 = Color3.fromRGB(20, 20, 23)
    Topbar.BorderColor3 = Color3.fromRGB(0, 0, 0)
    Topbar.BorderSizePixel = 0
    Topbar.Size = UDim2.new(0, 325, 0, 33)

    Frame_2.Parent = Topbar
    Frame_2.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    Frame_2.BorderColor3 = Color3.fromRGB(11, 56, 15)
    Frame_2.BorderSizePixel = 0
    Frame_2.Position = UDim2.new(0, 0, 1, 0)
    Frame_2.Size = UDim2.new(0, 325, 0, 1)

    TextLabel.Parent = Topbar
    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.BackgroundTransparency = 1.000
    TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
    TextLabel.BorderSizePixel = 0
    TextLabel.Size = UDim2.new(0, 286, 0, 33)
    TextLabel.Font = Enum.Font.Unknown
    TextLabel.Text = KName
    TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.TextSize = 12.000
    TextLabel.TextTransparency = 0.200
    TextLabel.TextXAlignment = Enum.TextXAlignment.Left

    UIPadding.Parent = TextLabel
    UIPadding.PaddingLeft = UDim.new(0, 10)

    Close.Name = "Close"
    Close.Parent = Topbar
    Close.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Close.BackgroundTransparency = 1.000
    Close.BorderColor3 = Color3.fromRGB(27, 42, 53)
    Close.LayoutOrder = -1
    Close.Position = UDim2.new(0.927671075, 0, 0.25, 0)
    Close.Size = UDim2.new(0, 16, 0, 16)
    Close.Image = "rbxassetid://6031094678"
    Close.ImageTransparency = 1.000

    Container.Name = "Container"
    Container.Parent = Frame
    Container.BackgroundColor3 = Color3.fromRGB(27, 27, 29)
    Container.BorderColor3 = Color3.fromRGB(0, 0, 0)
    Container.Position = UDim2.new(0, 0, 0.173000008, 0)
    Container.Size = UDim2.new(0, 325, 0, 150)

    TextLabel_2.Parent = Container
    TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel_2.BackgroundTransparency = 1.000
    TextLabel_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
    TextLabel_2.BorderSizePixel = 0
    TextLabel_2.Position = UDim2.new(0, 0, 0, 10)
    TextLabel_2.Size = UDim2.new(0, 325, 0, 26)
    TextLabel_2.Font = Enum.Font.SourceSans
    TextLabel_2.Text = "Info"
    TextLabel_2.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel_2.TextSize = 18.000
    TextLabel_2.TextWrapped = true

    TextboxHolder.Name = "Textbox Holder"
    TextboxHolder.Parent = Container
    TextboxHolder.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextboxHolder.BackgroundTransparency = 1.000
    TextboxHolder.Position = UDim2.new(0.0600000024, 0, 0.5, 0)
    TextboxHolder.Size = UDim2.new(0, 283, 0, 26)

    UICorner.Parent = TextboxHolder

    TextBox.Parent = TextboxHolder
    TextBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextBox.BackgroundTransparency = 1.000
    TextBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
    TextBox.Position = UDim2.new(0.00997352219, 0, 0, 0)
    TextBox.Size = UDim2.new(0, 282, 0, 26)
    TextBox.ClearTextOnFocus = false
    TextBox.Font = Enum.Font.SourceSans
    TextBox.PlaceholderColor3 = Color3.fromRGB(178, 178, 178)
    TextBox.PlaceholderText = "Enter the key..."
    TextBox.Text = ""
    TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextBox.TextSize = 14.000
    TextBox.TextStrokeTransparency = 0.000

    UIPadding_2.Parent = TextLabel_2
    UIPadding_2.PaddingLeft = UDim.new(0, 10)

    TextLabel_3.Parent = Container
    TextLabel_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel_3.BackgroundTransparency = 1.000
    TextLabel_3.BorderColor3 = Color3.fromRGB(0, 0, 0)
    TextLabel_3.Position = UDim2.new(0.75999999, 0, 0.670000017, 0)
    TextLabel_3.Size = UDim2.new(0, 78, 0, 22)
    TextLabel_3.Font = Enum.Font.SourceSans
    TextLabel_3.Text = ""
    TextLabel_3.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel_3.TextSize = 14.000
    TextLabel_3.TextWrapped = true

    JoinDiscord.Parent = Container
    JoinDiscord.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    JoinDiscord.BorderColor3 = Color3.fromRGB(11, 56, 15)
    JoinDiscord.BorderSizePixel = 0
    JoinDiscord.Position = UDim2.new(0.5, 0, 0.900000036, 0)
    JoinDiscord.Size = UDim2.new(0, 145, 0, 25)
    JoinDiscord.Font = Enum.Font.SourceSans
    JoinDiscord.Text = dislink
    JoinDiscord.TextColor3 = Color3.fromRGB(255, 255, 255)
    JoinDiscord.TextSize = 18.000
    JoinDiscord.TextWrapped = true

    UICorner_2.CornerRadius = UDim.new(0, 12)
    UICorner_2.Parent = JoinDiscord

    EnterKey.Name = "Enter Key"
    EnterKey.Parent = Container
    EnterKey.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    EnterKey.BorderColor3 = Color3.fromRGB(11, 56, 15)
    EnterKey.BorderSizePixel = 0
    EnterKey.Position = UDim2.new(0.0255078268, 0, 0.699980378, 0)
    EnterKey.Size = UDim2.new(0, 145, 0, 25)
    EnterKey.Font = Enum.Font.SourceSans
    EnterKey.Text = validKey
    EnterKey.TextColor3 = Color3.fromRGB(255, 255, 255)
    EnterKey.TextSize = 18.000
    EnterKey.TextWrapped = true

    UICorner_3.CornerRadius = UDim.new(0, 12)
    UICorner_3.Parent = EnterKey

    GetKey.Name = "Get Key"
    GetKey.Parent = Container
    GetKey.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    GetKey.BorderColor3 = Color3.fromRGB(11, 56, 15)
    GetKey.BorderSizePixel = 0
    GetKey.Position = UDim2.new(0.50999999, 0, 0.699980378, 0)
    GetKey.Size = UDim2.new(0, 145, 0, 25)
    GetKey.Font = Enum.Font.SourceSans
    GetKey.Text = "Get Key"
    GetKey.TextColor3 = Color3.fromRGB(255, 255, 255)
    GetKey.TextSize = 18.000
    GetKey.TextWrapped = true

    UICorner_4.CornerRadius = UDim.new(0, 12)
    UICorner_4.Parent = GetKey

    shadowHolder.Name = "shadowHolder"
    shadowHolder.Parent = Frame
    shadowHolder.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    shadowHolder.BackgroundTransparency = 1.000
    shadowHolder.Position = UDim2.new(0, 0, 0.839999974, 0)
    shadowHolder.Size = UDim2.new(0, 325, 0, 1)

    umbraShadow.Name = "umbraShadow"
    umbraShadow.Parent = shadowHolder
    umbraShadow.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    umbraShadow.BackgroundTransparency = 1.000
    umbraShadow.Size = UDim2.new(1, 0, 1, 6)
    umbraShadow.Image = "rbxassetid://1316045217"
    umbraShadow.ImageTransparency = 0.800

    penumbraShadow.Name = "penumbraShadow"
    penumbraShadow.Parent = shadowHolder
    penumbraShadow.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    penumbraShadow.BackgroundTransparency = 1.000
    penumbraShadow.Size = UDim2.new(1, 0, 1, 6)
    penumbraShadow.Image = "rbxassetid://1316045217"
    penumbraShadow.ImageTransparency = 0.800

    ambientShadow.Name = "ambientShadow"
    ambientShadow.Parent = shadowHolder
    ambientShadow.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ambientShadow.BackgroundTransparency = 1.000
    ambientShadow.Size = UDim2.new(1, 0, 1, 6)
    ambientShadow.Image = "rbxassetid://1316045217"
    ambientShadow.ImageTransparency = 0.800

    Frame.Active = true
    Frame.Draggable = true

    JoinDiscord.MouseButton1Click:Connect(function()
        game:GetService("HttpService"):GetAsync(Link)
    end)

    Close.MouseButton1Click:Connect(function()
        KeySystem:Destroy()
    end)

    local Debounce = false

    EnterKey.MouseButton1Click:Connect(function()
        if not Debounce then
            Debounce = true
            if TextBox.Text == validKey then
                TextLabel_3.Text = "Key Is Valid"
                TextLabel_3.TextColor3 = Color3.fromRGB(51, 204, 51)
                callback()
                wait(2)
                KeySystem:Destroy()
                Debounce = false
            else
                TextLabel_3.Text = "Key Is Invalid"
                TextLabel_3.TextColor3 = Color3.fromRGB(255, 51, 51)
                wait(2)
                TextLabel_3.Text = ""
                Debounce = false
            end
        end
    end)

    GetKey.MouseButton1Click:Connect(function()
        if whitelist then
            game:GetService("HttpService"):GetAsync("https://www.google.com/")
            game:GetService("HttpService"):GetAsync(Link)
        else
            KeySystem:Destroy()
        end
    end)

    return KeySystem
end

return Library
